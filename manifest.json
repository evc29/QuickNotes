{
  "manifest_version": 3,
  "name": "Quick Notes",
  "version": "1.0",
  "description": "Take quick notes and save them within the extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["storage"]
}
